    "start": "node ./server",
    "bundle": "webpack",
    "dev": "nodemon ./server --watch ./server"
  },
  "dependencies": {
    "argon2": "^0.15.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "bristol": "^0.3.3",
    "ejs": "^2.5.6",
    "express": "^4.14.1",
    "genesis-core": "technologyadvice/genesis-core",
    "http-errors": "^1.5.1",
    "istanbul": "^0.4.5",
    "jwt-simple": "^0.5.1",
    "mongodb": "^2.2.24",
    "obey": "^1.7.3",
    "promised-mongo": "^1.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-enroute": "^1.0.0",
    "redash": "^0.18.0",
    "request-ip": "^1.2.3",
    "require-dir": "^0.3.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "dirty-chai": "^1.2.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.11",
    "react-bootstrap": "^0.30.7",
    "react-hot-loader": "^1.3.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "standard": "^8.6.0",
    "webpack": "1.12.12",
    "webpack-dev-server": "^2.4.1"
  },
  "standard": {
    "ignore": [
      "coverage"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect",
      "should",
      "sinon",
      "proxyquire"
    ]
  }
}
